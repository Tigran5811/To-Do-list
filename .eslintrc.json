{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/prefer-stateless-function": "off",
        "linebreak-style": "off",
        "react/prop-types": "off",
        "no-shadow": "off",
        "react/button-has-type":"off"
    }
}